{
  "name": "resumate-root",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install-all": "npm --prefix server install && npm --prefix client install",
    "build": "npm --prefix client install && npm --prefix client run build",
    "start": "npm --prefix server start",
    "start:server": "npm --prefix server run server",
    "start:dev": "npm --prefix server run server",
    "server": "npm --prefix server run server",
    "client": "npm --prefix client run dev",
    "dev": "concurrently -k \"npm:server\" \"npm:client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
